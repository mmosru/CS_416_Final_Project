<h1>Sign up for W.O.C.A.</h1>


<%= form_for(@user) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation, "Confirmation" %>
  <%= f.password_field :password_confirmation %>
  <br />
  <%= f.submit "Sign up!" %>
<% end %>





<%= link_to 'Back', users_path %>
